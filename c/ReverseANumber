#include<stdio.h>
int main()
{
      int number, rev = 0,store, left;
      printf("Enter the number\n");
      scanf("%d", &number);

     store= number;
     
     while (number > 0)
     {
     
        left= number%10;
        rev = rev * 10 + left;
        number=number/10;

      }
      printf("Given number = %d\n",store);
      printf("Its reverse is = %d\n", rev);

     return 0;
}
